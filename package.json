  "description": "Manage your pull requests and issues from your command line.",
  "main": "index.prod.js",
  "keywords": [
    "git",
    "github",
    "gitlab",
    "command line",
    "cli",
    "cli interface",
    "node",
    "commander"
  ],
  "repository": "https://github.com/cezarlz/gitc",
  "author": "Cezar Luiz <cezarluiz.c@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.13.0",
    "git-branch": "^1.0.0",
    "git-url-parse": "^7.0.2",
    "opn": "^5.2.0",
    "parse-git-config": "^1.1.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  },
  "scripts": {
    "build": "babel index.js --out-file index.prod.js",
    "prepare": "npm run build"
